'''
x=0
for i in range(1,4):
    for j in range(1,4):
        if j==2 and i==1:
            print("  *")
        if (i==2 and j==1) or(i==2 and j==3):
          
            print("* ",end=' ')
            
        if i==3:
            
            if i==3 and j==1:
                if x<1 :
                    x=x+1
                    print("\n")
            print("*",end=" ")
